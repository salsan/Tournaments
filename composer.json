{
    "name": "salsan/tournaments",
    "description": "Tournaments is a PHP library designed to retrieve data about chess Tournaments from the Chess Federation.",
    "type": "library",
    "keywords": ["chess", "library", "Tournaments"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Salsan\\Tournaments\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Salvatore Santagati",
            "email": "github@salsan.dev"
        }
    ],
    "require": {
        "php": ">=8.1.10",
        "salsan/utils": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:salsan/PHPUtils.git"
        }
    ]

}
