    "keywords": ["chess", "library", "Tournaments"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Salsan\\Tournaments\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Salvatore Santagati",
            "email": "github@salsan.dev"
        }
